{
  "name": "goof-world",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:next\" \"npm run dev:server\"",
    "dev:next": "next dev",
    "dev:server": "tsx watch server/index.ts",
    "build": "next build",
    "start": "concurrently \"npm run start:next\" \"npm run start:server\"",
    "start:next": "next start",
    "start:server": "node dist/server/index.js",
    "build:server": "tsc -p tsconfig.server.json",
    "lint": "next lint"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "next": "^14.2.0",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "express": "^4.19.2",
    "zustand": "^4.5.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "typescript": "^5",
    "tsx": "^4.7.1",
    "concurrently": "^8.2.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.0"
  }
}
